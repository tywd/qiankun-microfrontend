{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VITE_USER_MANAGEMENT_URL": "https://qiankun-user-management.vercel.app",
    "VITE_SYSTEM_MANAGEMENT_URL": "https://qiankun-system-management.vercel.app"
  }
}